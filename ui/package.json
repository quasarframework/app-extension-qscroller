{
  "name": "@quasar/quasar-ui-qscroller",
  "version": "1.0.0-alpha.6",
  "author": "Jeff Galbraith <jeff@quasar.dev>",
  "description": "QScroller - Embed string, time, date and range scrollers in your Quasar Apps",
  "license": "MIT",
  "module": "dist/index.esm.js",
  "main": "dist/index.common.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "dev": "cd dev; yarn dev; cd ..",
    "dev:umd": "yarn build && node build/script.open-umd.js",
    "dev:ssr": "cd dev; yarn 'dev:ssr'; cd ..",
    "dev:ios": "cd dev; yarn 'dev:ios'; cd ..",
    "dev:android": "cd dev; yarn 'dev:android'; cd ..",
    "dev:electron": "cd dev; yarn 'dev:electron'; cd ..",
    "build": "node build/index.js",
    "build:js": "node build/script.javascript.js",
    "build:css": "node build/script.css.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quasarframework/quasar-ui-qscroller.git"
  },
  "bugs": "https://github.com/quasarframework/quasar-ui-qscroller/issues",
  "homepage": "https://github.com/quasarframework/quasar-ui-qscroller",
  "keywords": [
    "quasar",
    "quasarframework",
    "date",
    "time",
    "range",
    "date-time"
  ],
  "devDependencies": {
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "chalk": "^2.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-vue": "^6.0.0",
    "node-sass": "^4.13.0",
    "open": "^7.0.0",
    "postcss": "^7.0.21",
    "postcss-rtl": "^1.5.0",
    "quasar": "^1.3.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-es": "^3.3.9",
    "zlib": "^1.0.5"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "dependencies": {
    "q-colorize-mixin": "^1.0.0-alpha.3"
  }
}
